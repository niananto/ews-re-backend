{
  "openapi": "3.0.3",
  "info": {
    "title": "EWS-RE",
    "description": "Early Warning System for River Erosion",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://ews-re-back.onrender.com/",
      "variables": {},
      "description": "production server"
    },
    {
      "url": "http://localhost:8080/",
      "description": "local server"
    }
  ],
  "tags": [
    {
      "name": "data",
      "description": "everything about extracting data"
    }
  ],
  "paths": {
    "/link": {
      "get": {
        "tags": [
          "link"
        ],
        "summary": "fetch list of years, current year and its low, moderate and high water levels",
        "operationId": "fetchLink",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FetchLinkResponse"
                }
              }
            }
          }
        }
      }
    },
    "/link?year={year}": {
      "get": {
        "tags": [
          "link"
        ],
        "summary": "fetch the specified year's low, moderate and high water levels",
        "operationId": "fetchLinkYear",
        "parameters": [
          {
            "name": "year",
            "in": "query",
            "description": "Successful operation",
            "required": true,
            "schema": {
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FetchLinkYearResponse"
                }
              }
            }
          }
        }
      }
    },
    "/link?waterLevel={waterLevel}": {
      "get": {
        "tags": [
          "link"
        ],
        "summary": "fetch link of geojson file for a specific (closest) water level",
        "operationId": "fetchLinkWaterLevel",
        "parameters": [
          {
            "name": "waterLevel",
            "in": "query",
            "description": "Successful operation",
            "required": true,
            "schema": {
              "type": "number"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WaterLevelResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "FetchLinkResponse": {
        "type": "object",
        "properties": {
          "years": {
            "type": "list",
            "example": [2019, 2020, 2021]
          },
          "year": {
            "type": "number",
            "example": 2021
          },
          "low": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/annual_risks%2F1970_low.json?alt=media&token=cbc110e4-399f-4032-9031-6dd087e3bc68"
          },
          "mod": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/annual_risks%2F1970_mod.json?alt=media&token=6782ad98-acc1-42eb-b8ed-a2adfbcd3350"
          },
          "high": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/annual_risks%2F1970_high.json?alt=media&token=a6e363f9-c086-42e7-9792-883e4b653239"
          }
        }
      },
      "FetchLinkYearResponse": {
        "type": "object",
        "properties": {
          "year": {
            "type": "number",
            "example": 2021
          },
          "low": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/annual_risks%2F1970_low.json?alt=media&token=cbc110e4-399f-4032-9031-6dd087e3bc68"
          },
          "mod": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/annual_risks%2F1970_mod.json?alt=media&token=6782ad98-acc1-42eb-b8ed-a2adfbcd3350"
          },
          "high": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/annual_risks%2F1970_high.json?alt=media&token=a6e363f9-c086-42e7-9792-883e4b653239"
          }
        }
      },
      "WaterLevelResponse": {
        "type": "object",
        "properties": {
          "water_level": {
            "type": "number",
            "example": 16.0087
          },
          "url": {
            "type": "string",
            "example": "https://firebasestorage.googleapis.com/v0/b/ews-re-9d81a.appspot.com/o/water-level-marked%2F16.00874640032320.json?alt=media&token=222ba32a-58e2-4ece-862f-88a928d72485"
          }
        }
      }
    }
  }
}